#include <stdio.h>

int main() {
    int numbers[100], size, i;
    int evenCount = 0, oddCount = 0;
    scanf("%d", &size);
    printf("Enter %d elements:\n", size);
    for(i = 0; i < size; i++) {
        scanf("%d", &numbers[i]);
    }
    for(i = 0; i < size; i++) {
        if(numbers[i] % 2 == 0)
            evenCount++;
        else
            oddCount++;
    }
    printf("Even numbers: %d\n", evenCount);
    printf("Odd numbers: %d\n", oddCount);

    return 0;
}

